<%- include("partials/header"); -%>
<h1>Home</h1>
<%= content %>
<% for (let post of posts) { %>
    <div class="my-2">
        <h2><%= post.title %> </h2>
        <p><%= post.postBody.substring(0, 100) + "..." %> <a href="/posts/<%= post.title.replace(" ", "-") %>">Read More</a> </p>

    </div>
<% } %>
<%- include("partials/footer"); -%> 